    "type": "library",
    "license": "MIT",
    "require": {
        "laravel/framework": ">=5.1.0",
        "php": ">=5.5.9"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tecbeast42/basic-auth-middleware"
        }
    ],
    "autoload": {
        "psr-4": {
            "TecBeast\\BasicAuthWithEnv\\": "src/"
        }
    }
}
